default:os_startup.xsm init.xsm int1.xsm int2.xsm int3.xsm int4.xsm int5.xsm int6.xsm int7.xsm exhandler.xsm timer.xsm x1.xsm x2.xsm
os_startup.xsm:os.spl
	$(spl)/spl --os os.spl
init.xsm:init.apl
	$(apl)/apl init.apl
int1.xsm:int1.spl
	$(spl)/spl --int=1 int1.spl
int2.xsm:int2.spl
	$(spl)/spl --int=2 int2.spl
int3.xsm:int3.spl
	$(spl)/spl --int=3 int3.spl
int4.xsm:int4.spl
	$(spl)/spl --int=4 int4.spl
int5.xsm:int5.spl
	$(spl)/spl --int=5 int5.spl
int6.xsm:int6.spl
	$(spl)/spl --int=6 int6.spl
int7.xsm:int7.spl
	$(spl)/spl --int=7 int7.spl
exhandler.xsm:exhandler.spl
	$(spl)/spl --exhandler exhandler.spl
timer.xsm:timer.spl
	$(spl)/spl --int=timer timer.spl
x1.xsm:x1.apl
	$(apl)/apl x1.apl	
x2.xsm:x2.apl
	$(apl)/apl x2.apl		
